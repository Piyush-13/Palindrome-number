package test;

import java.util.Arrays;

public class Array {

	public static void main(String[] args) {
		int num = 101;
		int rev =0;
		int original = num;
		
		while(num!=0) {
			
			int digit = num%10;
			 rev = rev*10 + digit;
			 num/=10;
			
			
		}
		
		if(original==rev) {
			System.out.println("palindrome");
		}
		else {
			System.out.println("Not palindrome");
		}
		
	}
}




